{
  "name": "zanai-code",
  "displayName": "ZanaiCode",
  "description": "Superpílula para KiloCode - Crie e gerencie agentes de IA",
  "version": "1.0.0",
  "publisher": "ZanaiCode",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Machine Learning",
    "AI"
  ],
  "keywords": [
    "ai",
    "agent",
    "kilocode",
    "copilot",
    "assistant",
    "llm"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "zanaiCode.openDashboard",
        "title": "ZanaiCode: Abrir Dashboard",
        "category": "ZanaiCode"
      },
      {
        "command": "zanaiCode.downloadAgent",
        "title": "ZanaiCode: Baixar Agente",
        "category": "ZanaiCode"
      },
      {
        "command": "zanaiCode.listAgents",
        "title": "ZanaiCode: Listar Agentes",
        "category": "ZanaiCode"
      },
      {
        "command": "zanaiCode.refreshAgents",
        "title": "ZanaiCode: Atualizar Agentes",
        "category": "ZanaiCode"
      },
      {
        "command": "zanaiCode.showHelp",
        "title": "ZanaiCode: Ajuda",
        "category": "ZanaiCode"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "zanaiCode.openDashboard",
          "when": "true"
        },
        {
          "command": "zanaiCode.downloadAgent",
          "when": "true"
        },
        {
          "command": "zanaiCode.listAgents",
          "when": "true"
        },
        {
          "command": "zanaiCode.refreshAgents",
          "when": "true"
        },
        {
          "command": "zanaiCode.showHelp",
          "when": "true"
        }
      ]
    },
    "configuration": {
      "title": "ZanaiCode",
      "properties": {
        "zanaiCode.serverUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "URL do servidor ZanaiCode"
        },
        "zanaiCode.autoUpdate": {
          "type": "boolean",
          "default": true,
          "description": "Atualizar agentes automaticamente"
        },
        "zanaiCode.debugMode": {
          "type": "boolean",
          "default": false,
          "description": "Ativar modo de depuração"
        }
      }
    }
  }
}